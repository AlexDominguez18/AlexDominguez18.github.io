---
import PROJECTS from '../data/projects.js';
import ProjectItem from './ProjectItem.astro';

---

<div class="flex flex-col gap-y-16">
  {
    PROJECTS.map((project) => (
      <article class="flex flex-col space-x-0 space-y-2 group">
        <ProjectItem {...project} />
      </article>
    ))
  }
</div>